      "API",
      "Gateway"
   ],
   "author": "James R. Gisler",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/jgisler/aws-multiregion-api/issues"
   },
   "homepage": "https://github.com/jgisler/aws-multiregion-api#readme",
   "devDependencies": {
      "config": "^3.1.0",
      "js-yaml": "^3.13.1"
   }
}
